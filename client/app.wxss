
/**app.wxss**/

@import "./style/iconfont.wxss";
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
.user-info{
  display: flex;
  align-items: center;
  height: 120rpx;
  background: #fff;
}
.user-head{
  flex-shrink: 0;
  margin-left: 20rpx;
  height: 60rpx;
  width: 60rpx;
  background: #f5e069;
  border-radius: 50%;
}