page {
		display: block;
		min-height: 100%;
		background-color: rgb(245, 245, 247);
		font-size: 46rpx;
	}
	
	.payed-wrapper {
		display: flex;
		flex-direction: column;
		align-items: center;
		background: #3396fb;
		box-sizing: border-box;
		height: 180rpx;
		color: #fff;
	}
	
	.current-profit {
		margin: 20rpx 0;
	}
	
	
	.text-center{
		text-align: center;
	}
	
	.i-cell-bd{
		font-size: 40rpx;
	}
	
	.unpayed-wrapper {
		height: 150rpx;
		background: #fff;
		color: #0d4872;
	}
	
	.current-unincome,
	.current-unfreight {
		justify-content: center;
		height: 150rpx;
		float: left;
		width: 50%;
		display: flex;
		flex-direction: column;
		align-items: center;
		box-sizing: border-box;
	}
	
	.current-unincome {
		border-right: 1px solid #f0f0f0;
	}
	
	.list-wrapper {
		margin-top: 20rpx;
		color: #0d4872;
	}
	
	.item-wrapper {
		overflow: hidden;
		text-align: center;
		background: #fff;
		border-bottom: 1px solid #eee;
	}
	
	.item-wrapper,
	.item-tit,
	.item-more {
		height: 120rpx;
		line-height: 120rpx;
	}
	
	.item-tit {
		float: left;
		width: 75%;
		padding-left: 5%;
		text-align: left;
	}
	
	.item-more {
		float: left;
		width: 20%;
	}
	
	.icon-transport,
	.icon-account-in,
	.icon-freight,
	.icon-expend {
		font-size: 22px;
	}