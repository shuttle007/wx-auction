page {
	display: block;
	min-height: 100%;
	font-size: 46rpx;
	background-color: rgb(245, 245, 247);
}

.list-wrapper {
	margin-top: 30rpx;
	width: 100%;
	overflow: hidden;
}
.companies-wrapper{
	overflow:hidden;
	background: #fff;
