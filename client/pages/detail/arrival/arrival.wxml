<view class="detail-wrapper">
	<view class="item-wrapper">
		<view class="item-title">到货时间</view>
		<view class="item-detail">{{item.date}}</view>
	</view>
	<view class="item-wrapper">
		<view class="item-title">到达客户</view>
		<view class="item-detail">{{item.company}}</view>
	</view>
	<view class="item-wrapper">
		<view class="item-title">车牌号码</view>
		<view class="item-detail">{{item.truck_license}}</view>
	</view>
	<view class="item-wrapper">
		<view class="item-title">卸货吨数</view>
		<view class="item-detail">{{item.after_amount}}吨</view>
	</view>
	<!-- 编辑和删除 -->
	<view class='edit-del-wrapper'>
		<view class='edit-btn' bindtap='navToEdit'>编辑</view>
		<view class='delete-btn' bindtap="showDelModal">删除</view>
	</view>
</view>